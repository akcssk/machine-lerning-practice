    "# 二次方程式の解\n",
    "import numpy as np\n",
    "def qeq(a, b, c):\n",
    "    d = np.sqrt(b**2 - 4 * a * c)\n",
    "    return ((-b +d) / (2 * a)  ,(-b - d) / (2 * a))"
